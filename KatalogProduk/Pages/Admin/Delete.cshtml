@page
@model KatalogProduk.Pages.Admin.DeleteModel
@{
    ViewData["Title"] = "Delete";
}

<h3>Are you sure you want to delete this User?</h3>
<hr />
<div>
    <dl class="row">
        <dt class="col-sm-2">
            Nama Lengkap
        </dt>
        <dd class="col-sm-10">
            @Model.fullname
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.email
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="@Model.id2" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-page="./User">Back to List</a>
    </form>
</div>
